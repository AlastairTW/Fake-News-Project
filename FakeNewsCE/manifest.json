{
   "author": "Cameron Wragg",
   "browser_action": {
      "default_icon": "images/icon.png",
      "default_popup": "popup.html"
   },
   "content_scripts": [ {
      "js": [ "js/jquery.min.js", "js/popup.js", "js/material.min.js" ],
      "css": [ "css/style.css" , "css/material.min.css"],
      "matches": [ "<all_urls>" ]
   } ],
   "description": "Fake News Extension for voting on the integrity of articles.",
   "icons": {
      "128": "images/iconStore.png",
      "16": "images/icon.png",
      "48": "images/iconLauncher.png"
   },
   "manifest_version": 2,
   "name": "FakeNewsCE",
   "permissions": [ "activeTab" ],
   "version": "0.0.1",
   "background": {
     "scripts": ["js/background.js"]
   }
}
